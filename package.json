    "store": "^2.0.12",
    "vue": "^2.6.11",
    "vue-clipboard2": "^0.2.1",
    "vue-i18n": "^8.17.4",
    "vue-router": "^3.3.4",
    "vue-svg-component-runtime": "^1.0.1",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@ant-design/colors": "^3.2.1",
    "@vue/cli-plugin-babel": "^4.0.4",
